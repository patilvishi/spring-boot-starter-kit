<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>{"time":"%d{yyyy-MM-dd HH:mm:ss}", "level":"%level", "logger":"%logger{36}", "msg":"%msg"}%n</pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
